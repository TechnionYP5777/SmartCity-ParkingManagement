<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<StackPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.driver.app.ChooseParkingSlotController">
   <children>
      <HBox prefHeight="100.0" prefWidth="1200.0">
         <children>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="400.0">
               <children>
                  <TableView fx:id="slotsTable" prefHeight="520.0" prefWidth="400.0">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </TableView>
               </children></HBox>
            <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #3b5998;">
               <children>
                  <WebView fx:id="myWebView" prefHeight="520.0" prefWidth="0.0" />
                  <Button fx:id="addMarkersButton" mnemonicParsing="false" onAction="#addMarkers" text="add markers">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="changeMarkersButton" mnemonicParsing="false" onAction="#changeMarker" text="change Marker">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</StackPane>
