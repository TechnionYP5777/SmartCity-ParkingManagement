<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXDatePicker?>
<?import com.jfoenix.controls.JFXRadioButton?>
<?import com.jfoenix.controls.JFXTimePicker?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<StackPane prefHeight="850.0" prefWidth="1250.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.driver.app.ChooseParkingSlotController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <VBox prefHeight="310.0" prefWidth="100.0">
               <children>
                  <HBox alignment="BOTTOM_CENTER" prefHeight="80.0" prefWidth="200.0">
                     <children>
                        <Label text="Order Parking Slot">
                           <font>
                              <Font name="System Bold" size="36.0" />
                           </font>
                           <HBox.margin>
                              <Insets bottom="5.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
                  <Label text="1.Choose Destination">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="15.0" top="15.0" />
                     </VBox.margin>
                  </Label>
                  <HBox alignment="TOP_CENTER" prefHeight="60.0" prefWidth="200.0">
                     <children>
                        <JFXRadioButton text="Student House">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </JFXRadioButton>
                        <JFXRadioButton text="Ulman">
                           <toggleGroup>
                              <ToggleGroup fx:id="destination" />
                           </toggleGroup>
                        </JFXRadioButton>
                        <JFXRadioButton text="Taub" toggleGroup="$destination" />
                        <JFXRadioButton text="Sports Center" toggleGroup="$destination" />
                     </children>
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                  </HBox>
                  <Label text="2.Choose Date &amp; Time">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="15.0" />
                     </VBox.margin>
                  </Label>
                  <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="200.0">
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                     <children>
                        <JFXDatePicker promptText="At date" />
                        <JFXTimePicker promptText="Arrival time" />
                        <JFXTimePicker promptText="Departure time" />
                     </children>
                  </HBox>
                  <Label text="3.Choose Parking Slot">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="15.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
            <HBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="1250.0">
               <children>
                  <HBox alignment="TOP_CENTER" maxHeight="-Infinity" prefHeight="520.0" prefWidth="410.0">
                     <children>
                        <TableView fx:id="slotsTable" maxHeight="-Infinity" prefHeight="502.0" prefWidth="400.0">
                           <HBox.margin>
                              <Insets top="7.0" />
                           </HBox.margin>
                        </TableView>
                     </children>
                  </HBox>
                  <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="810.0">
                     <children>
                        <WebView fx:id="myWebView" prefHeight="520.0" prefWidth="0.0" />
                        <Button fx:id="addMarkersButton" mnemonicParsing="false" onAction="#addMarkers" text="add markers">
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
