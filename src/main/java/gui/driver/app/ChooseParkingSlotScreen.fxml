<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<StackPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.driver.app.ChooseParkingSlotController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: #3b5998;">
         <children>
            <WebView fx:id="myWebView" prefHeight="400.0" prefWidth="200.0" />
            <Button fx:id="addMarkersButton" mnemonicParsing="false" onAction="#addMarkers" text="add markers">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="removeMarkersButton" mnemonicParsing="false" onAction="#deleteMarkers" text="delete markers">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</StackPane>
